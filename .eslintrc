{
"extends": [
    "react-app",
    "airbnb",
    "plugin:jsx-a11y/recommended",
    "prettier",
    ],
    "plugins": [
        "jsx-a11y",
        "prettier"
        ],
        "rules": {
            "react/sort-comp": "warn",
            "react/destructuring-assignment": "warn",
            "react/button-has-type": "warn",
            "semi":0,
            "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
            "prettier/prettier": [
                 "error", {
                    "semi": false,
                    "endOfLine":"auto"
            }
        ]
     }
}